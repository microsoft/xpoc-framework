{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "moduleResolution": "Node",
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}
